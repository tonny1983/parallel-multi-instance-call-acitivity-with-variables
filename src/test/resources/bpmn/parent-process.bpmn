    <bpmn:scriptTask id="collect-variables-from-subprocess" name="collect variables from subprocess" camunda:asyncBefore="true" scriptFormat="javascript">
      <bpmn:incoming>Flow_0und4sv</bpmn:incoming>
      <bpmn:outgoing>Flow_1aewjqp</bpmn:outgoing>
      <bpmn:script>var subprocesses = execution.getProcessEngineServices().getHistoryService().createHistoricProcessInstanceQuery().superProcessInstanceId(execution.getProcessInstanceId()).list();

var system = java.lang.System;

var total = 0;

for (var i = 0; i &lt; subprocesses.size(); i++) {
var subprocess = subprocesses.get(i);
var id = subprocess.getId();
var variable = execution.getProcessEngineServices().getHistoryService().createHistoricVariableInstanceQuery().processInstanceId(id).variableName("subVariable").singleResult();

var value = variable.getValue();

system.out.println(id + ":" + value);
total += value;
}

execution.setVariable("anotherTotal", total);


</bpmn:script>
    </bpmn:scriptTask>
